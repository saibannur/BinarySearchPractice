def binarySearchCount(arr, n, key):
 
    left = 0
    right = n - 1
 
    count = 0
 
    while (left < right):
        mid = int((right + left) / 2)
 
        # Check if middle element is
        # less than or equal to key
        if (arr[mid] < key and arr[mid +1 ] > key):
 
            # At least (mid + 1) elements are there
            # whose values are less than
            # or equal to key
            count = mid + 1
            left = mid + 1
        elif arr[mid] == key:
            count = mid
            left = mid
            
        elif arr[mid] < key and arr[mid+1] < key:
            count = mid + 2
            left = mid + 1
        else:
            right = mid - 1
     
    return count


val = input()
val = int(val)
incre = 0

allSoaps = input()

x = allSoaps.split()

for i in range(0, len(x)):
    x[i] = int(x[i])
x.sort()  


numOfQueries = int(input())


while incre < numOfQueries:
 
  queries = int(input())
 
  print( binarySearchCount(x, len(x), queries))
  
  incre += 1
